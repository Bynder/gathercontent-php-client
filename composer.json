{
    "name": "gathercontent/client",
    "description": "GatherContent client",
    "type": "library",
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "bin-dir": "bin",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4.0"
        }
    },
    "require": {
        "guzzlehttp/guzzle": "^6.3  | ^7",
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "^3.7",
        "webmozart/path-util": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "GatherContent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GatherContent\\Composer\\": "src-dev/Composer",
            "GatherContent\\Tests\\": "src-dev/Tests"
        }
    }
}
